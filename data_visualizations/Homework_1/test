var x = location.search;

x=x.replace(/[^A-Za-z]/g, "");
x="afaffff"
var dict = []; 
for (i = 0; i < x.length; i++) { 

if (x[i] in  dict) {
    dict[x[i]]++;
} else {
    dict[x[i]]=1;
}

};
var ff=[];
for (var key in dict ){
document.write(key);
ff.push({label:key, count:dict[key]});
}
document.write(ff[1]['count']);



